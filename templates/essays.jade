doctype html

html
  head
    if(title)
      title S. C. Young - #{title}
    else
      title S. C. Young

    include includes/css.jade
    include includes/analytics.jade

  body
    include includes/sidebar.jade
    #content
      h1 Essays
      for x in essays
        h2
          a(href='/#{x.path}') #{x.title}
        .subtitle #{x.prettydate}


